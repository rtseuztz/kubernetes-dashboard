{
	"name": "myapp",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "npm run prepare && vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"prepare": "npm run smui-theme",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@smui/button": "^7.0.0-beta.8",
		"@smui/data-table": "^7.0.0-beta.8",
		"@smui/drawer": "^7.0.0-beta.8",
		"@smui/list": "^7.0.0-beta.8",
		"@smui/tooltip": "^7.0.0-beta.8",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@types/node": "^20.2.4",
		"smui-theme": "^7.0.0-beta.8",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"dependencies": {
		"cli-table-2-json": "^1.0.13"
	}
}
